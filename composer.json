{
    "name": "p365labs/html2csv",
    "description": "A simple project to manage Html <table> and convert them to CSV file",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Federico Panini",
            "email": "fpanini@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.11.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "vendor-dir": "vendor",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
